model CarDetail {
  id             String @id @default(uuid())
  brand          String
  model          String
  year           String
  vehicleType    String
  color          String
  mileage        String
  engineCapacity String
  carSegments    String
  driveType      String
  engine         String
  transmission   String
  fuelType       String
  vinCode        String
  brakeSystem    String
  insurancePolicy String?
  frontImage     String
  backImage      String
  insideImage    String
  othersImage    String[]
  auction        Auction  @relation("AuctionCarDetail", fields: [auctionId], references: [id], onDelete:Cascade)
  auctionId      String  @unique 

 }